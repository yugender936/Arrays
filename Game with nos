class Complete {

    // Function for finding maximum and value pair
    public static int[] game_with_number(int arr[], int n) {
        // Complete the function
        int[] nums = new int[n];
        for(int i = 0 ; i < arr.length - 1; i++){
            nums[i] = arr[i]^arr[i+1];
        }
        nums[n-1] = arr[n-1];
        return nums;
    }
}
