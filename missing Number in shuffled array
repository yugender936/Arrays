class Solution {

    public int findMissing(int[] arr1, int[] arr2) {
        // code here
        int sum1 = 0;
        int sum2 = 0;
        for(int i = 0 ; i < arr1.length ; i++){
            if(i < arr1.length){
                sum1 = sum1+arr1[i];
            }
            if(i < arr2.length){
                sum2 = sum2+arr2[i];
            }
        }
        
        return sum1-sum2;
        
        
    }
}
